<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd">
    <name>sap.ushell_abap</name>
    <vendor>SAP SE</vendor>
    <version>1.88.1</version>
    <copyright>Copyright (c) 2009-2020 SAP SE, All Rights Reserved</copyright>
    <documentation>SAP library: sap.ushell_abap</documentation>

    <dependencies>
        <dependency>
            <libraryName>sap.ui.core</libraryName>
        </dependency>
        <dependency>
            <libraryName>sap.m</libraryName>
        </dependency>
        <dependency>
            <libraryName>sap.ushell</libraryName>
        </dependency>
        <dependency>
            <libraryName>sap.ui.layout</libraryName>
            <lazy>true</lazy>
        </dependency>
        <dependency>
            <libraryName>sap.ui.fl</libraryName>
            <lazy>true</lazy>
        </dependency>
    </dependencies>

    <!-- Exclude plugins from preload file -->
    <appData>
        <packaging xmlns="http://www.sap.com/ui5/buildext/packaging" version="2.0">
            <all-in-one>
                <exclude name="sap/ushell_abap/plugins/" />
                <exclude name="sap/ushell_abap/support/plugins/" />
            </all-in-one>
        </packaging>
        <!-- excludes for the JSDoc -->
        <jsdoc xmlns="http://www.sap.com/ui5/buildext/jsdoc" >
            <exclude name="sap.ui.Device" />
            <exclude name="sap.ushell_abap.bootstrap.abap" />
            <exclude name="sap.ushell_abap.bootstrap.appruntime-*" />
            <exclude name="sap.ushell_abap.bootstrap.evo.core-*" />
        </jsdoc>
        <ownership xmlns="http://www.sap.com/ui5/buildext/ownership">
            <component>CA-FLP-FE-UI</component><!-- default component for library -->
            <!--Fiori Configuration Cockpit (FCC) Transport UI -->
            <component>
                <name>CA-FLP-ABA</name>
                <modules>
                    <module>sap/ushell_abap/plugins/fcc-transport-ui/*</module>
                </modules>
            </component>
        </ownership>
    </appData>
</library>