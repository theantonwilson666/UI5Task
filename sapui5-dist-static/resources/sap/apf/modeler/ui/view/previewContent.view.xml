<mvc:View controllerName="sap.apf.modeler.ui.controller.previewContent"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" class="sapUiSizeCompact">
	<Dialog xmlns="sap.m" type="Standard" id="idPreviewContentDialog"
		 class="previewDialogStyle" afterClose="handleClose">
		<VBox xmlns="sap.m" id="idThumbnail" height="240px" width="33%"
			initialFocus="true" displayInline="false" direction="Column" fitContainer="false"
			renderType="Div" justifyContent="Center" alignItems="Center"
			class="previewThumbnailHolder">
			<items>
			</items> <!-- sap.ui.core.Control -->
		</VBox>
		<VBox xmlns="sap.m" id="idMainChart" width="67%" displayInline="false"
			direction="Column" fitContainer="false" renderType="Div"
			justifyContent="Center" alignItems="Center"
			class="previewTable previewMainChartHolder">
			<items></items> <!-- sap.ui.core.Control -->
		</VBox>
		<endButton>
			<Button xmlns="sap.m" id="idCloseButtonDialog" press="handleCloseButtonOfDialog">
			</Button>
		</endButton>
	</Dialog>
</mvc:View>