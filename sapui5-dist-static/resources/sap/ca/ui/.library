<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd">

    <name>sap.ca.ui</name>
    <vendor>SAP SE</vendor>
    <version>1.88.0</version>
    <copyright>SAP UI development toolkit for HTML5 (SAPUI5) (c) Copyright 2009-2014 SAP SE. All rights reserved</copyright>

    <documentation>SAP UI library: Fiori Commons</documentation>

    <deprecation since="1.28">
All controls and helpers in this library meanwhile have been deprecated.
Check the individual classes or packages for actual replacements.
    </deprecation>

    <dependencies>
        <dependency>
            <libraryName>sap.ui.core</libraryName>
        </dependency>
        <dependency>
            <libraryName>sap.m</libraryName>
        </dependency>
    </dependencies>

    <appData>
    	<packaging xmlns="http://www.sap.com/ui5/buildext/packaging" version="2.0">
			<module-infos>
				<raw-module name="sap/ca/ui/utils/exif.js"
					requiresTopLevelScope="false" />
		  	</module-infos>
		</packaging>
        <selenium xmlns="http://www.sap.com/ui5/buildext/selenium" package="sap.ca.selenium.ui"/>
        <documentation xmlns="http://www.sap.com/ui5/buildext/documentation" indexUrl="../../../../test-resources/sap/ca/ui/demokit/docuindex.json" resolve="lib"/>
    </appData>

</library>
