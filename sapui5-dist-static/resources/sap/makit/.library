<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd" >

	<name>sap.makit</name>
	<vendor>SAP SE</vendor>
	<copyright>SAPUI5

(c) Copyright 2009-2021 SAP SE. All rights reserved</copyright>
	<version>1.88.0</version>

	<documentation>Mobile Chart controls based on the Sybase MAKIT charting lib.</documentation>

	<deprecation since="1.38">
MAKIT charts have been replaced with sap.viz and VizFrame in 1.38. This library will not be supported anymore from 1.38.
	</deprecation>

	<dependencies>
		<dependency>
			<libraryName>sap.ui.core</libraryName>
		</dependency>
	</dependencies>

	<appData>
		<selenium xmlns="http://www.sap.com/ui5/buildext/selenium" package="com.sap.ui5.selenium.makit" />
		<jsdoc xmlns="http://www.sap.com/ui5/buildext/jsdoc" >
			<exclude name="sap.makit.js." />
		</jsdoc>
		<!-- excludes for the JSCoverage -->
		<jscoverage xmlns="http://www.sap.com/ui5/buildext/jscoverage" >
			<exclude name="sap.makit.js." />
		</jscoverage>
		<packaging xmlns="http://www.sap.com/ui5/buildext/packaging" version="2.0" >
			<all-in-one>
				<!-- ===========================================================================
				     !! ATTENTION !!
				     The following excludes must be kept in sync with those configured for the
				     'preload-and-merge' plugin execution in the pom.xml of this library.
				     =========================================================================== -->
				<exclude name="sap/makit/js/" />
				<exclude name="sap/makit/localization/" />
			</all-in-one>
		</packaging>
		<releasenotes xmlns="http://www.sap.com/ui5/buildext/releasenotes"
			url="../../../test-resources/sap/makit/relnotes/changes-{major}.{minor}.json"
			resolve="lib" />
	</appData>

</library>
